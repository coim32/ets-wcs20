<?xml version="1.0" encoding="UTF-8"?>
<!-- Add this entry to TE_BASE/config.xml (//organization/standard) -->
<organization>
  <name>OGC</name>
  <standard>
    <name>Web Coverage Service (WCS)</name>
    <version>
      <name>2.0.1</name>
      <suite>
        <namespace-uri>http://www.opengis.net/wcs/2.0</namespace-uri>
        <prefix>wcs2</prefix>
        <local-name>ets-wcs20</local-name>
        <title>WCS 2.0 Conformance Test Suite</title>
        <description>Checks WCS Core implementations for conformance with OGC 09-110r3 and 09-110r4.</description>
      </suite>
      <revision>
        <name>${project.version}</name>
        <sources>
          <source>wcs/${spec-version}/ctl</source>
        </sources>
        <resources>wcs/${spec-version}/resources</resources>
        <webdir>wcs/${spec-version}/site</webdir>
      </revision>
    </version>
  </standard>
</organization>
